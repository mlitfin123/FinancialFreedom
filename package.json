{
  "name": "financialfreedom",
  "version": "1.0.0",
  "description": "basic API app ##Dependencies - Chart.js - Dotenv",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RogerLamotheJR/financialFreedom.git"
  },
  "author": "roger lamothe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RogerLamotheJR/financialFreedom/issues"
  },
  "homepage": "https://github.com/RogerLamotheJR/financialFreedom#readme",
  "dependencies": {
    "apexcharts": "^3.20.0",
    "axios": "^0.20.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "es7": "^1.0.8-beta.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "lite-server": "^2.5.3",
    "moment": "^2.27.0",
    "mongoose": "^5.10.3",
    "morgan": "^1.10.0",
    "node": "^14.8.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  }
}
